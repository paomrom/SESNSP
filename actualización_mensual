viofam_2015<-base_2015 %>% group_by(Municipio) %>% 
  filter(`Tipo de delito`== "Violencia familiar",
         Clave_Ent==14) %>%
  summarise(ene=sum(Enero),
            feb=sum(Febrero),
            mar=sum(Marzo),
            abr=sum(Abril),
            may=sum(Mayo),
            jun=sum(Junio),
            jul=sum(Julio),
            ago=sum(Agosto),
            sep=sum(Septiembre),
            oct=sum(Octubre),
            nov=sum(Noviembre),
            dic=sum(Diciembre),
            Total_2015=sum(ene+feb+mar+abr+ 
                             may+jun+jul+ago+
                             sep+oct+nov+dic)) %>% 
  select(Municipio, `Total_2015`)


